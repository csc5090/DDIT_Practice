<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.our_middle_project.dao.MemberDAO">

    <select id="getTotalUserCount" resultType="int">
        SELECT COUNT(*) FROM MEMBER
    </select>
    
    <select id="getNewUserCountToday" resultType="int">
    	SELECT COUNT(*) FROM MEMBER WHERE TRUNC(CREATED_DATE) = TRUNC(SYSDATE)
    </select>

</mapper>